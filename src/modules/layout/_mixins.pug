



//- type: `jpg`/ `png` or jpg/ png if so-called var inited
//- src: `${path}/to/img`  - without prefixes and dimensions
//- dimentions: `s`/ `sml`/ `sl`/ `smlx`/ `sx`....
//- retina: true/ false

//- Атрибуты типа class, alt записываются как обычно в скобках сразу за миксином  -- +picture(jpg, 'tmp')(class="pic" alt="lev")

//- image nameing: anyname-sm.jpg

- var jpg = `jpg`, png = `png`

- var globalRetinaUsage = `` //- `` -if false
- var globalDimentionsUsage = `sml`

- var postfixRetina = '@r'
- var postfix = { sm: `-sm`, md: `-md`, lg: `-lg`, xl: `-xl` } //- `smlx`

mixin picture(type, src, dimentions = globalDimentionsUsage, retina = globalRetinaUsage)
	picture
		if dimentions.includes(`n`)
			source(srcset=ipl data-srcset=`${retina.includes(`n`) ? `${src + postfixRetina}.webp 2x,` : ``} ${src}.webp 1x` type=`image/webp`)
			source(srcset=ipl data-srcset=`${retina.includes(`n`) ? `${src + postfixRetina}.${type} 2x,` : ``} ${src}.${type} 1x` type=`image/${type}`)
			img.lazyload(src=ipl data-src=`${src}.${type}` alt='')&attributes(attributes)
		else
			if dimentions.includes(`x`)
				source(media=`(min-width: ${xl}px)` srcset=ipl data-srcset=`${retina.includes(`x`) ? `${src + postfix.xl + postfixRetina}.webp 2x,` : ``} ${src + postfix.xl}.webp 1x` type=`image/webp`)
				source(media=`(min-width: ${xl}px)` srcset=ipl data-srcset=`${retina.includes(`x`) ? `${src + postfix.xl + postfixRetina}.${type} 2x,` : ``} ${src + postfix.xl}.${type} 1x` type=`image/${type}`)
			if dimentions.includes(`l`)
				source(media=`(min-width: ${lg}px)` srcset=ipl data-srcset=`${retina.includes(`l`) ? `${src + postfix.lg + postfixRetina}.webp 2x,` : ``} ${src + postfix.lg}.webp 1x` type=`image/webp`)
				source(media=`(min-width: ${lg}px)` srcset=ipl data-srcset=`${retina.includes(`l`) ? `${src + postfix.lg + postfixRetina}.${type} 2x,` : ``} ${src + postfix.lg}.${type} 1x` type=`image/${type}`)
			if dimentions.includes(`m`)
				source(media=`(min-width: ${md}px)` srcset=ipl data-srcset=`${retina.includes(`m`) ? `${src + postfix.md + postfixRetina}.webp 2x,` : ``} ${src + postfix.md}.webp 1x` type=`image/webp`)
				source(media=`(min-width: ${md}px)` srcset=ipl data-srcset=`${retina.includes(`m`) ? `${src + postfix.md + postfixRetina}.${type} 2x,` : ``} ${src + postfix.md}.${type} 1x` type=`image/${type}`)
			if dimentions.includes(`s`)
				source(media=`(min-width: 0px)` srcset=ipl data-srcset=`${retina.includes(`s`) ? `${src + postfix.sm + postfixRetina}.webp 2x,` : ``} ${src + postfix.sm}.webp 1x` type=`image/webp`)
				source(media=`(min-width: 0px)` srcset=ipl data-srcset=`${retina.includes(`s`) ? `${src + postfix.sm + postfixRetina}.${type} 2x,` : ``} ${src + postfix.sm}.${type} 1x` type=`image/${type}`)
			if dimentions.includes(`l`)
				img.lazyload(src=ipl data-src=`${src + postfix.lg}.${type}` alt='')&attributes(attributes)
			else
				img.lazyload(src=ipl data-src=`${src + postfix.md}.${type}` alt='')&attributes(attributes)
			
mixin picture-min(type, src, minSrc,  dimentions = globalDimentionsUsage, retina = globalRetinaUsage)
	picture
		if dimentions.includes(`n`)
			source(srcset=`${minSrc}-min.${type}` data-srcset=`${retina.includes(`n`) ? `${src + postfixRetina}.webp 2x,` : ``} ${src}.webp 1x` type=`image/webp`)
			source(srcset=`${minSrc}-min.${type}` data-srcset=`${retina.includes(`n`) ? `${src + postfixRetina}.${type} 2x,` : ``} ${src}.${type} 1x` type=`image/${type}`)
			img.lazyload(src=`${minSrc}-min.${type}` data-src=`${src}.${type}` alt='')&attributes(attributes)
		else
			if dimentions.includes(`x`)
				source(media=`(min-width: ${xl}px)` srcset=`${minSrc + postfix.xl}-min.${type}` data-srcset=`${retina.includes(`x`) ? `${src + postfix.xl + postfixRetina}.webp 2x,` : ``} ${src + postfix.xl}.webp 1x` type=`image/webp`)
				source(media=`(min-width: ${xl}px)` srcset=`${minSrc + postfix.xl}-min.${type}` data-srcset=`${retina.includes(`x`) ? `${src + postfix.xl + postfixRetina}.${type} 2x,` : ``} ${src + postfix.xl}.${type} 1x` type=`image/${type}`)
			if dimentions.includes(`l`)
				source(media=`(min-width: ${lg}px)` srcset=`${minSrc + postfix.lg}-min.${type}` data-srcset=`${retina.includes(`l`) ? `${src + postfix.lg + postfixRetina}.webp 2x,` : ``} ${src + postfix.lg}.webp 1x` type=`image/webp`)
				source(media=`(min-width: ${lg}px)` srcset=`${minSrc + postfix.lg}-min.${type}` data-srcset=`${retina.includes(`l`) ? `${src + postfix.lg + postfixRetina}.${type} 2x,` : ``} ${src + postfix.lg}.${type} 1x` type=`image/${type}`)
			if dimentions.includes(`m`)
				source(media=`(min-width: ${md}px)` srcset=`${minSrc + postfix.md}-min.${type}` data-srcset=`${retina.includes(`m`) ? `${src + postfix.md + postfixRetina}.webp 2x,` : ``} ${src + postfix.md}.webp 1x` type=`image/webp`)
				source(media=`(min-width: ${md}px)` srcset=`${minSrc + postfix.md}-min.${type}` data-srcset=`${retina.includes(`m`) ? `${src + postfix.md + postfixRetina}.${type} 2x,` : ``} ${src + postfix.md}.${type} 1x` type=`image/${type}`)
			if dimentions.includes(`s`)
				source(media=`(min-width: 0px)` srcset=`${minSrc + postfix.sm}-min.${type}` data-srcset=`${retina.includes(`s`) ? `${src + postfix.sm + postfixRetina}.webp 2x,` : ``} ${src + postfix.sm}.webp 1x` type=`image/webp`)
				source(media=`(min-width: 0px)` srcset=`${minSrc + postfix.sm}-min.${type}` data-srcset=`${retina.includes(`s`) ? `${src + postfix.sm + postfixRetina}.${type} 2x,` : ``} ${src + postfix.sm}.${type} 1x` type=`image/${type}`)
			if dimentions.includes(`l`)
				img.lazymin.lazyload(src=`${minSrc + postfix.lg}-min.${type}` data-src=`${src + postfix.lg}.${type}` alt='')&attributes(attributes)
			else
				img.lazymin.lazyload(src=`${minSrc + postfix.md}-min.${type}` data-src=`${src + postfix.md}.${type}` alt='')&attributes(attributes)
//- Quiz items
mixin quiz-radio(text, value = null)
	a.quiz__radio.quiz-radio(data-value=`${value == null ? text : value}`)&attributes(attributes)
		.quiz__radio-toggler
			.quiz__radio-toggler--inner
		p.quiz__radio-value!= text
mixin quiz-radio-final(text, value = null)
	a.quiz__radio.quiz-radio-final(data-value=`${value == null ? text : value}`)&attributes(attributes)
		.quiz__radio-toggler
			.quiz__radio-toggler--inner
		p.quiz__radio-value!= text

mixin quiz-option(text, value = null)
	a.quiz__radio.quiz-option(data-value=`${value == null ? text : value}`)&attributes(attributes)
		.quiz__radio-toggler
			.quiz__radio-toggler--inner
		p.quiz__radio-value!= text

mixin quiz-nav(count)
	nav.quiz__nav.quiz-final-hide
		.quiz__progress
			.quiz__progress-title Шаг #[span.quiz-progress-num 1] #[=lang[62][1]] #[= count]
			ul.quiz__progress-nav.quiz-progress-nav.uk-dotnav.slidedots.slidedots
				- for(var i = 1; i <= count; i++)
					li(class=`${i == 1 ? 'uk-active' : ''}`)
						a
		.quiz__btns
			button.quiz-btn-prev.uk-button.btn-outline-primary.d-none.d-md-flex
				span Предыдущий шаг
			button.quiz-btn-next.uk-button.btn-solid-primary
				span Следующий шаг

mixin quiz-slide-first(idx)
	.quiz-slide.quiz-slide--first(data-title=lang[13])
		h3=lang[13]
		.quiz-radio-wrap.quiz__radio-wrap
			+quiz-radio(lang[63][0], `${idx != 1 ? `#quiz-1` : ''}`, `${assets}s3-i1`)(class=`${idx == 1 ? 'quiz-active' : ''} quiz__radio--sm`)
			+quiz-radio(lang[63][1], `${idx != 2 ? `#quiz-2` : ''}`, `${assets}s3-i2`)(class=`${idx == 2 ? 'quiz-active' : ''} quiz__radio--sm`)
			+quiz-radio(lang[63][2], `${idx != 3 ? `#quiz-3` : ''}`, `${assets}s3-i3`)(class=`${idx == 3 ? 'quiz-active' : ''} quiz__radio--sm`) 
			+quiz-radio(lang[63][3], `${idx != 4 ? `#quiz-4` : ''}`, `${assets}s3-i4`)(class=`${idx == 4 ? 'quiz-active' : ''} quiz__radio--sm`)

mixin quiz-slide-dimentions()
	.quiz-slide.quiz-validate(data-title=lang[64])
		h3= lang[64]
		.input-wrap 
			input.quiz__input.input(type="text" name="Площадь" placeholder=`${lang[65]}, м2`)
		.input-wrap 
			input.quiz__input.input(type="tel" name="Телефон" placeholder="Ваш телефон")
		+picture(jpg, `${modules}quiz-dim`, 'sl', 'l')(class="quiz__img-dim")

mixin quiz-slide-final()
	.quiz-slide.quiz-slide-final&attributes(attributes)
		form(action="#")
			p.quiz__final-title Укажите номер телефона для связи
			p.quiz__final-subtitle!= lang[68]
			.input-wrap
				input.quiz__final-input.input(type="tel" name="Телефон" placeholder="Введите номер телефона")
			button.quiz__final-submit.uk-button.btn-solid-primary(type="submit") Отправить
			+picture(jpg, `${modules}popup-tel`, 'n', 'n')(class="quiz__img-tel")
			.values.d-none


//- Slider dots and navigation

mixin slider-dots()
	ul.uk-slider-nav.uk-dotnav.slidedots&attributes(attributes)
mixin slideshow-dots()
	ul.uk-slideshow-nav.uk-dotnav.slidedots&attributes(attributes)

mixin slider-nav()
	a.slidenav.slidenav-prev(href="#" data-uk-slider-item="previous")&attributes(attributes)
		+icon('angle-left', primary)
	a.slidenav.slidenav-next(href="#" data-uk-slider-item="next")&attributes(attributes)
		+icon('angle-right', primary)

mixin slideshow-nav()
	a.slidenav.slidenav-prev(href="#" data-uk-slideshow-item="previous")&attributes(attributes)
		+icon('angle-left')
	a.slidenav.slidenav-next(href="#" data-uk-slideshow-item="next")&attributes(attributes)
		+icon('angle-right')

